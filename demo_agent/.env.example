# credentials for sdk authentication
# copy this file to .env and fill in your values

# required for demo ui
OPENAI_API_KEY=your-openai-api-key-here

# optional: only needed if using github sdk
GITHUB_TOKEN=your-github-token-here

# optional: only needed if using azure sdks
AZURE_TENANT_ID=your-azure-tenant-id
AZURE_SUBSCRIPTION_ID=your-azure-subscription-id

# optional: only needed if using aws boto3
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-east-1

# optional: only needed if using stripe
STRIPE_API_KEY=your-stripe-api-key

# kubernetes uses ~/.kube/config automatically (no env vars needed)
